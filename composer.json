{
  "name": "made/blog",
  "description": "Simple blog software for developers.",
  "type": "project",
  "minimum-stability": "stable",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.3 <7.4",
    "ext-json": "*",
    "slim/slim": "^4.0",
    "slim/psr7": "^0.6.0",
    "pimple/pimple": "^3.2",
    "slim/twig-view": "^3.0",
    "monolog/monolog": "^2.0",
    "gameplayjdk/php-file-cache": "^1.2.3",
    "gameplayjdk/pimple-package-utility": "^1.1",
    "gameplayjdk/php-static-help": "^1.0",
    "made/blog-engine": "dev-content-repository",
    "made/blog-theme-base": "dev-content-repository"
  },
  "authors": [
    {
      "name": "GameplayJDK",
      "email": "github@gameplayjdk.de"
    },
    {
      "name": "Dennis Barlowe",
      "email": "dennis@dennzo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "setup": "php vendor/made/blog-theme-base/setup.php"
  },
  "repositories": [
    {
      "type": "path",
      "url": "package/made/blog-engine"
    },
    {
      "type": "path",
      "url": "package/made/blog-theme-base"
    }
  ]
